{
  "name": "egapro-next",
  "version": "1.21.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "type-check": "tsc --pretty --noEmit",
    "fresh-install": "rm -rf node_modules/ build/ && yarn",
    "check-all": "yarn lint && yarn type-check && yarn test --watchAll=false",
    "dev:up": "docker-compose up --build",
    "dev:prod": "docker-compose -f docker-compose.build.yaml up --build",
    "postinstall": "[ -n .env ] && cp .env.sample .env || true",
    "test:api": "docker exec egapro-next_api_1 /usr/local/bin/py.test -vv --cov"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.5",
    "@chakra-ui/react": "^1.8.3",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/cabin": "^4.5.2",
    "@fontsource/gabriela": "^4.5.2",
    "@socialgouv/matomo-next": "^1.2.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "date-fns": "^2.28.0",
    "final-form": "^4.20.6",
    "framer-motion": "^6",
    "jest": "^27.5.1",
    "moize": "^6.1.0",
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-final-form": "^6.5.8",
    "react-icons": "^4.3.1",
    "swr": "^1.2.1"
  },
  "devDependencies": {
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "17.0.17",
    "@types/react": "17.0.39",
    "eslint": "8.9.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-testing-library": "^5.0.5",
    "prettier": "^2.5.1",
    "typescript": "4.5.5"
  }
}
